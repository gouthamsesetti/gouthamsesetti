def isprime(n):
     for i in range(2,n):
            if n%i==0:
                 return True
     return False
t=int(input('enter t value'))
for i in range(t):   
    n=int(input('enter n value'))
    count=0
    for i in range(2,n):
                   if isprime(i):
                       count=count+1
print(count)
           
